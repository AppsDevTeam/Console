	"require": {
		"nette/nette": "@dev",
		"nette/application": "@dev",
		"nette/bootstrap": "@dev",
		"nette/caching": "@dev",
		"nette/component-model": "@dev",
		"nette/database": "@dev",
		"nette/di": "@dev",
		"nette/finder": "@dev",
