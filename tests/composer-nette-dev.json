		"nette/reflection": "@dev",
		"nette/robot-loader": "@dev",
		"nette/safe-stream": "@dev",
		"nette/security": "@dev",
		"nette/tokenizer": "@dev",
		"nette/utils": "@dev",
		"latte/latte": "@dev",
		"tracy/tracy": "@dev",

		"symfony/console": "~2.4"
	},
	"require-dev": {
		"nette/tester": "@dev",
		"kdyby/events": "@dev",
		"symfony/event-dispatcher": "~2.4",
		"jakub-onderka/php-parallel-lint": "~0.6"
	},
	"autoload": {
		"psr-0": {
